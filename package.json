{
  "name": "@kakarot-ci/core",
  "version": "0.15.0",
  "description": "Core package for Kakarot CI - AI-powered unit test generation for pull requests",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "kakarot-ci": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:clean": "rm -rf dist",
    "build:esm": "node build.js esm",
    "build:cjs": "node build.js cjs",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "npm run build:esm -- --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "release": "semantic-release",
    "prepare": "husky",
    "yalc:publish": "npm run build && yalc publish --push",
    "yalc:push": "npm run build && yalc push"
  },
  "keywords": [
    "ci",
    "testing",
    "jest",
    "typescript",
    "ai",
    "llm",
    "pull-request",
    "automation"
  ],
  "author": "David Jansen",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "commander": "^14.0.2",
    "cosmiconfig": "^9.0.0",
    "find-up": "^8.0.0",
    "git-url-parse": "^16.1.0",
    "minimatch": "^10.1.1",
    "simple-git": "^3.30.0",
    "typescript": "^5.3.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.6.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.54.0",
    "husky": "^9.1.7",
    "semantic-release": "^22.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.6.1",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
